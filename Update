/* Mobile (<576px) */
@media (max-width: 575.98px) {
  .time-travel-wrapper {
    width: 100%;
    margin-bottom: 0.75rem;
  }
}

/* Tablet (‚â•576px and <992px) */
@media (min-width: 576px) and (max-width: 991.98px) {
  .time-travel-wrapper {
    flex: 1;
    min-width: 280px;
  }
}

/* Laptop & Desktop (‚â•992px) */
@media (min-width: 992px) {
  .time-travel-wrapper {
    min-width: 340px;
    margin-bottom: 0;
  }
}
      `}</style>
    </>
  );
}

export default function WrappedApp() {
  return (
    <BrowserRouter>
      <div className="dark-theme">
        <App />
      </div>
    </BrowserRouter>
  );
}
















